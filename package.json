{
  "name": "samu-api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "GPL-2.0",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && ./node_modules/copyfiles/copyfiles -u 1 \"**/*.hbs\" dist/src",
    "start": "nodemon src/main.ts",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "testLocal": "jest --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/typeormConfig.ts",
    "deploy": "npm run build && ./node_modules/.bin/now --token=$NOW_TOKEN",
    "generate-services": "ts-node src/swagger/swaggerBuilder.ts && openapi-generator generate -g typescript-inversify -i swagger-spec.json -o ../salvemundi.nl/src/openapi --additional-properties=\"useRxJS6=true\""
  },
  "dependencies": {
    "@mollie/api-client": "^3.0.0-beta.1",
    "@nestjs/common": "^6.8.2",
    "@nestjs/core": "^6.8.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/platform-express": "^6.8.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/nodemailer": "^6.2.1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "0.10.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "mysql": "^2.17.1",
    "nestjs-config": "^1.4.4",
    "nodemailer": "^6.3.1",
    "nodemailer-express-handlebars": "^3.1.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.19",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.8.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.14.20",
    "@types/supertest": "^2.0.8",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.6",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "^5.20.0",
    "typescript": "3.4.3",
    "wait-on": "^3.3.0"
  }
}
