    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.2",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "mysql": "^2.17.1",
    "nestjs-config": "^1.4.2",
    "now": "^16.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.6",
