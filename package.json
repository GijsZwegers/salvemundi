{
  "name": "@salvemundi/samu-api",
  "version": "2.4.1-beta18",
  "description": "Salve Mundi API",
  "author": "SV Salve Mundi",
  "license": "GPL-2.0",
  "repository": {
    "url": "https://github.com/salvemundi/samu-api"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && ./node_modules/copyfiles/copyfiles -u 1 \"**/*.hbs\" dist/src",
    "start": "nodemon src/main.ts",
    "test": "CI=true ./node_modules/.bin/jest --coverage --coverageReporters=text-lcov | coveralls",
    "testLocal": "CI=true ./node_modules/.bin/jest --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/typeormConfig.ts",
    "generate-services": "ts-node src/swagger/swaggerBuilder.ts && ./node_modules/@openapitools/openapi-generator-cli/bin/openapi-generator generate -g typescript-inversify -i swagger-spec.json -o ../salvemundi.nl/src/openapi --additional-properties=\"useRxJS6=true\""
  },
  "dependencies": {
    "@mollie/api-client": "^3.0.0",
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/nodemailer": "^6.4.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "class-transformer": "^0.2.3",
    "class-validator": "0.11.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "nest-schedule": "^0.6.4",
    "nestjs-config": "^1.4.5",
    "node-rsa": "^1.0.7",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "sha.js": "^2.4.11",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "0.2.20",
    "cli-highlight": "2.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.14",
    "@openapitools/openapi-generator-cli": "^1.0.7-4.2.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/supertest": "^2.0.8",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "24.2.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "^5.20.1",
    "typescript": "3.7.4",
    "wait-on": "^3.3.0"
  }
}
