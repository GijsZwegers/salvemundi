    "jest": "^24.5.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "^5.18.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
