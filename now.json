{
    "version": 2,
    "name": "SaMu-api",
    "build": {
      "env": {
        "SERVER_PORT": "@server_port",
        "JWT_SECRET": "@jwt_secret",
        "DB_HOST": "@db_host",
        "DB_PORT": "@db_port",
        "DB_USER": "@db_user",
        "DB_PASSWORD": "@db_password",
        "DB_NAME": "@db_name"
      }
    },
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@now/node-server"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ]
  }